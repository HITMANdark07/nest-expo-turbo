{
  "name": "nest-expo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "check-types": "turbo run check-types",
    "start": "turbo run start",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "api:dev": "turbo run api:dev --filter=api",
    "api:build": "turbo run build --filter=api",
    "api:start": "turbo run start --filter=api",
    "prisma:studio": "turbo run db:studio --filter=@repo/db",
    "prisma:generate": "turbo run db:generate --filter=@repo/db",
    "prisma:migrate:dev": "turbo run db:migrate --filter=@repo/db",
    "prisma:seed": "turbo run db:seed --filter=@repo/db",
    "expo:android": "turbo run android --filter=expo-app",
    "expo:ios": "turbo run ios --filter=expo-app",
    "expo:web": "turbo run web --filter=expo-app",
    "expo:dev": "turbo run dev --filter=expo-app",
    "admin:dev": "turbo run dev --filter=admin",
    "admin:build": "turbo run build --filter=admin",
    "admin:start": "turbo run start --filter=admin",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "create-expo-app": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-turbo": "^1.12.3",
    "expo-cli": "^6.3.10",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2",
    "typescript-eslint": "^7.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
