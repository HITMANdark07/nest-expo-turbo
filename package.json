{
  "name": "nest-expo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "start": "turbo run start",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "api:dev": "turbo run dev --filter=api",
    "api:build": "turbo run build --filter=api",
    "api:start": "turbo run start --filter=api",
    "expo:android": "turbo run android --filter=expo",
    "expo:ios": "turbo run ios --filter=expo",
    "expo:web": "turbo run web --filter=expo",
    "expo:dev": "turbo run dev --filter=expo"
  },
  "devDependencies": {
    "create-expo-app": "^3.3.0",
    "expo-cli": "^6.3.10",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
